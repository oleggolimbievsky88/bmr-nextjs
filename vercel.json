{
    "build": {
      "env": {
        "NODE_ENV": "preview"
      }
    },
    "rewrites": [
      { "source": "/api/:path*", "destination": "/app/api/:path*" }
    ],
      "git": {
        "deploymentEnabled": {
          "main": true,
          "staging": true
        }
      },
      "github": {
        "enabled": true,
        "silent": true
      }
    }